@prefix :        <http://mmisw.org/ont/mmitest/udunits2-prefixes/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix u2:      <http://mmisw.org/ont/mmitest/udunits2/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

:pico
      a       u2:Prefix ;
      u2:hasSymbol "p" ;
      u2:hasValue "1e-12" .

:hecto
      a       u2:Prefix ;
      u2:hasSymbol "h" ;
      u2:hasValue "100" .

u2:hasCardinality
      rdfs:domain u2:UnitName ;
      rdfs:range <http://www.w3.org/2001/XMLSchema#string> .

:zepto
      a       u2:Prefix ;
      u2:hasSymbol "z" ;
      u2:hasValue "1e-21" .

:femto
      a       u2:Prefix ;
      u2:hasSymbol "f" ;
      u2:hasValue "1e-15" .

u2:Unit
      a       owl:Class ;
      rdfs:label "Unit" .

:giga
      a       u2:Prefix ;
      u2:hasSymbol "G" ;
      u2:hasValue "1e9" .

:tera
      a       u2:Prefix ;
      u2:hasSymbol "T" ;
      u2:hasValue "1e12" .

:zetta
      a       u2:Prefix ;
      u2:hasSymbol "Z" ;
      u2:hasValue "1e21" .

:deci
      a       u2:Prefix ;
      u2:hasSymbol "d" ;
      u2:hasValue ".1" .

:milli
      a       u2:Prefix ;
      u2:hasSymbol "m" ;
      u2:hasValue "1e-3" .

:nano
      a       u2:Prefix ;
      u2:hasSymbol "n" ;
      u2:hasValue "1e-9" .

:exa  a       u2:Prefix ;
      u2:hasSymbol "E" ;
      u2:hasValue "1e18" .

:centi
      a       u2:Prefix ;
      u2:hasSymbol "c" ;
      u2:hasValue ".01" .

u2:hasDefinition
      a       owl:FunctionalProperty ;
      rdfs:domain u2:Unit ;
      rdfs:range <http://www.w3.org/2001/XMLSchema#string> .

:yotta
      a       u2:Prefix ;
      u2:hasSymbol "Y" ;
      u2:hasValue "1e24" .

u2:hasAlias
      rdfs:domain u2:Unit ;
      rdfs:range u2:UnitName .

u2:namesUnit
      a       owl:FunctionalProperty ;
      rdfs:domain u2:UnitName ;
      rdfs:range u2:Unit .

u2:hasPrefixName
      rdfs:domain u2:Prefix ;
      rdfs:range <http://www.w3.org/2001/XMLSchema#string> .

:peta
      a       u2:Prefix ;
      u2:hasSymbol "P" ;
      u2:hasValue "1e15" .

u2:Prefix
      a       owl:Class ;
      rdfs:label "Prefix" .

:deka
      a       u2:Prefix ;
      u2:hasSymbol "da" ;
      u2:hasValue "10" .

u2:hasValue
      a       owl:FunctionalProperty ;
      rdfs:domain u2:Prefix ;
      rdfs:range <http://www.w3.org/2001/XMLSchema#string> .

:yocto
      a       u2:Prefix ;
      u2:hasSymbol "y" ;
      u2:hasValue "1e-24" .

u2:hasSymbol
      rdfs:domain u2:UnitName ;
      rdfs:range <http://www.w3.org/2001/XMLSchema#string> .

u2:hasName
      a       owl:FunctionalProperty ;
      rdfs:domain u2:Unit ;
      rdfs:range u2:UnitName .

u2:UnitName
      a       owl:Class ;
      rdfs:label "UnitName" .

:mega
      a       u2:Prefix ;
      u2:hasSymbol "M" ;
      u2:hasValue "1e6" .

:atto
      a       u2:Prefix ;
      u2:hasSymbol "a" ;
      u2:hasValue "1e-18" .

:kilo
      a       u2:Prefix ;
      u2:hasSymbol "k" ;
      u2:hasValue "1e3" .

:micro
      a       u2:Prefix ;
      u2:hasSymbol "u" , "μ" , "µ" ;
      u2:hasValue "1e-6" .
